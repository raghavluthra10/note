{
   "env": {
      "node": true,
      "commonjs": true,
      "es2021": true
   },
   "extends": "eslint:recommended",
   "overrides": [],
   "parserOptions": {
      "ecmaVersion": "latest"
   },
   "rules": {
      "semi": ["warn", "always"],
      "quotes": ["warn", "double"],
      "no-multiple-empty-lines": ["error", { "max": 2, "maxEOF": 0 }]
   }
}
